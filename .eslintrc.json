{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:@typescript-eslint/recommended",
        "prettier"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": "latest",
        "parser": "@typescript-eslint/parser"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
        "vue/multi-word-component-names": "off",
        "no-console": "off",
        "@typescript-eslint/no-non-null-assertion": 1
    },
    "root": true
}
